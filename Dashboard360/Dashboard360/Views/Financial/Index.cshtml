@section CustomScripts {
    @Scripts.Render("~/Scripts/Financial/getAverageSalesPrice.js")
    @Scripts.Render("~/Scripts/Financial/getAccountsReceivable.js")
    @Scripts.Render("~/Scripts/Financial/getAccountsPayable.js")
}
<div class="col-lg-12 col-md-12 col-xs-12" style="text-align: center;">
    <h1>Financial View </h1>
</div>
<div class="row">
    <!-- Total Turnover -->
    <div class="col col-lg-4 col-md-4">
        <h3> Total Turnover </h3>
    </div>

    <!-- Total Expenses -->
    <div class="col col-lg-4 col-md-4">
        <h3> Total Expenses </h3>
    </div>

    <!-- Average Sales Price -->
    <div class="col col-lg-4 col-md-4">
        <h3> Average Sales Price </h3>
        <span class="averageSalesPrice"></span>
    </div>

    <!-- Accounts Payable -->
    <div class="col col-lg-4 col-md-4">
        <h3> Accounts Payable </h3>
        <span class="accountsPayable"></span>
    </div>

    <!-- Accounts Receivable -->
    <div class="col col-lg-4 col-md-4">
        <h3> Accounts Receivable </h3>
        <span class="accountsReceivable"></span>
    </div>

    <!-- Cash Flow -->
    <div class="col col-lg-4 col-md-4">
        <h3> Cash Flow </h3>
    </div>

    <!-- Total Assets -->
    <div class="col col-lg-4 col-md-4">
        <h3> Total Assets </h3>
    </div>

    <!-- Total Liabilities -->
    <div class="col col-lg-4 col-md-4">
        <h3> Total Liabilities </h3>
    </div>

    <!-- Sales Revenue Growth -->
    <div class="col col-lg-4 col-md-4">
        <h3> Sales Revenue Growth </h3>
    </div>
</div>
